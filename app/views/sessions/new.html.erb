<h1>Log in</h1>

<%= form_tag sessions_path do %>
  <div class="field">
    <%= text_field_tag :email, nil, :placeholder => 'Enter your email' %>
  </div>
  <div class="field">
    <%= password_field_tag :password, nil, :placeholder => 'Enter your email' %>
  </div>
  <div class="actions"><%= submit_tag "Log in" %></div>
<% end %>
